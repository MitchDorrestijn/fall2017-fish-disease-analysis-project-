service cloud.firestore {
  	match /databases/{database}/documents {
    	match /{document=**} {
      		allow read, write;
    	}

		match /aquaria/{aquarium=**} {
			// Rules go here...
			allow write, read: if get(/databases/$(database)/documents/aquaria/$(projectID)).user.id == request.auth.uid;
		}
  	}
}
