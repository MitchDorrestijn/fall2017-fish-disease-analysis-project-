  "dependencies": {
    "bootstrap": "^4.0.0-beta",
    "firebase": "^4.6.2",
    "fuse.js": "^3.2.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "react": "^16.1.1",
